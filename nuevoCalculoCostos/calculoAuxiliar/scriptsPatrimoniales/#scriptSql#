DELETE t1 FROM contacts t1
INNER JOIN contacts t2 
WHERE 
    t1.id < t2.id AND 
    t1.email = t2.email;

DELETE t1 FROM PRHISTORICOS t1
INNER JOIN PRHISTORICOS t2 
WHERE 
    t1.prhID < t2.prhID AND 
    t1.RUBRO = t2.RUBRO AND
    t1.CATEGORIA = t2.CATEGORIA AND
    t1.MODELO = t2.MODELO AND
    t1.VIGENCIA = t2.VIGENCIA;
