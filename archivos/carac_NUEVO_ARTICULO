(nartID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
RUBRO INT NOT NULL,
CATEGORIA INT NOT NULL,
MODELO INT NOT NULL,
FECHA DATE,
REALIZACION DATE,
UNIQUE index_articulos (RUBRO,CATEGORIA,MODELO),
			  CONSTRAINT `RUBROna`
					FOREIGN KEY (RUBRO) REFERENCES RUBRO (ruID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `CATEGORIAna`
					FOREIGN KEY (CATEGORIA) REFERENCES CATEGORIA (claID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `MODELOna`
					FOREIGN KEY (MODELO) REFERENCES MODELO (mdID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



