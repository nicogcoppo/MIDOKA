(scID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
REFERENCIA INT NOT NULL,
CLASE INT NOT NULL,
ASIGNADA INT,
COMIENZO DATE,
REALIZACION DATE,

			  CONSTRAINT `REFERENCIA SOLICITUD`
					FOREIGN KEY (REFERENCIA) REFERENCES OPERACIONES (opID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `CLASE DE SOLICITUD`
					FOREIGN KEY (CLASE) REFERENCES ORDEN_OPERACION (oopID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `ASIGNADA A`
					FOREIGN KEY (ASIGNADA) REFERENCES RECURSOS_HUMANOS_DISPONIBLES (rhdID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



