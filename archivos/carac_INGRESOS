(ingID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
INGRESA INT NOT NULL,
CANTIDAD INT NOT NULL,
REFERENCIA_ID INT NOT NULL,
UNIQUE index_INGRESOS (INGRESA,REFERENCIA_ID),
			  CONSTRAINT `COMPRA REFERENCIA`
					FOREIGN KEY (REFERENCIA_ID) REFERENCES SALDO_PROOVEDOR (REFERENCIA)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT

							      
							      ) ENGINE = InnoDB;
		



