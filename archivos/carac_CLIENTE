(clID INT NOT NULL AUTO_INCREMENT, 
DENOMINACION VARCHAR(100),
DIRECCION VARCHAR(100),
LOCALIDAD INT NOT NULL,
PROVINCIA INT NOT NULL,
HORARIO VARCHAR(60),
TEL_A VARCHAR(60),
TEL_B VARCHAR(60),
TEL_C VARCHAR(60),
CONDICION INT NOT NULL,
LATITUD DOUBLE,
LONGITUD DOUBLE,
NACIONALIDAD INT NOT NULL,
COMERCIO INT NOT NULL,
TIPO INT NOT NULL,
PERSONA VARCHAR(60),
QQ VARCHAR(30),
WECHAT VARCHAR(60),
EMAIL VARCHAR(100),
RECOMIENDA VARCHAR(100),
NOTAS DOUBLE,
UNIQUE index_deno (DENOMINACION),
UNIQUE index_cliente (DIRECCION,LOCALIDAD,PROVINCIA,LATITUD,LONGITUD),
PRIMARY KEY (clID),
			  CONSTRAINT `CONDICION`
					FOREIGN KEY (CONDICION) REFERENCES CONDICION (cdID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `ESTAFETA`
					FOREIGN KEY (LOCALIDAD) REFERENCES LOCALIDAD (lcID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `PROVINCIAS`
					FOREIGN KEY (PROVINCIA) REFERENCES PROVINCIA (prID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `NACIONALIDAD`
					FOREIGN KEY (NACIONALIDAD) REFERENCES NACIONALIDAD (ncID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `COMERCIOS`
					FOREIGN KEY (COMERCIO) REFERENCES COMERCIO (coID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `TIPO CLIENTE`
					FOREIGN KEY (TIPO) REFERENCES TIPO (tcID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



