(opID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
INTERESADO INT NOT NULL,
SOLICITUD INT NOT NULL,
FECHA DATE,
COMPLETADO DATE,
			  CONSTRAINT `CLIENTE INTERESADO`
					FOREIGN KEY (INTERESADO) REFERENCES CLIENTE (clID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `TIPO DE SOLICITUD`
					FOREIGN KEY (SOLICITUD) REFERENCES NOMBRE_MENU (nmID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



