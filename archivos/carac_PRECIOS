(precioID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
PROOVEDOR INT NOT NULL,
TIPO_COMISION INT NOT NULL,
RUBRO_pr INT NOT NULL,
CATEGORIA_pr INT NOT NULL,
MODELO_pr INT NOT NULL,
PRECIO_A DOUBLE,
VIGENCIA_A DATE,
PRECIO_B DOUBLE,
VIGENCIA_B DATE,
DESCRIPCION_CHINO VARCHAR (100),
CODIGO_BARRAS INT,
CANTIDAD_MINIMA INT,
CANTIDAD_BULTO INT,
DESCUENTO_ESPECIFICO DOUBLE,
CODIGO_PROOVEDOR INT,
VOLUMEN_CM3 DOUBLE,
DESCUENTO_ESPECIAL DOUBLE,
CONDICION_REFERENCIA INT,
UMBRAL INT,
REGALO INT,
UNIQUE index_precio (RUBRO_pr,CATEGORIA_pr,MODELO_pr),
			  CONSTRAINT `RUBRO_PRECIO`
					FOREIGN KEY (RUBRO_pr) REFERENCES ARTICULOS (RUBRO)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `CATEGORIA_pr`
					FOREIGN KEY (CATEGORIA_pr) REFERENCES ARTICULOS (CATEGORIA)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `MODELO_pr`
					FOREIGN KEY (MODELO_pr) REFERENCES ARTICULOS (MODELO)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `PROOVEDOR_pr`
					FOREIGN KEY (PROOVEDOR) REFERENCES PROOVEDOR (pooID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `TIPO_COMISION_pr`
					FOREIGN KEY (TIPO_COMISION) REFERENCES TIPO_COMISION (tcoID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT						     
							      
							      ) ENGINE = InnoDB;
