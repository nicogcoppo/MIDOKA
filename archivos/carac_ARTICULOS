(artID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
RUBRO INT NOT NULL,
CATEGORIA INT NOT NULL,
MODELO INT NOT NULL,
MOTIVO INT NOT NULL,
UNIQUE index_articulos (RUBRO,CATEGORIA,MODELO,MOTIVO),
			  CONSTRAINT `RUBRO`
					FOREIGN KEY (RUBRO) REFERENCES RUBRO (ruID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `CATEGORIA`
					FOREIGN KEY (CATEGORIA) REFERENCES CATEGORIA (claID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `MODELO`
					FOREIGN KEY (MODELO) REFERENCES MODELO (mdID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `MOTIVO`
					FOREIGN KEY (MOTIVO) REFERENCES MOTIVO (mtID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



