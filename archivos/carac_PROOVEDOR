(pooID INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
PROVINCIA INT NOT NULL,
NOMBRE_COMERCIAL VARCHAR(100),
DIRECCION_OFICINA VARCHAR(100),
DIRECCION_DEPOSITO VARCHAR(100),
DESCUENTO DOUBLE,
VALOR_AGREGADO DOUBLE,
TELEFONO_A VARCHAR(60),
TELEFONO_B VARCHAR(60),
TELEFONO_C VARCHAR(60),
CORREO VARCHAR(100),
NOTA DOUBLE,
			  CONSTRAINT `PROVINCIA PROOVEDOR`
					FOREIGN KEY (PROVINCIA) REFERENCES PROVINCIA (prID)
						     ON UPDATE RESTRICT

							      ) ENGINE = InnoDB;
		



