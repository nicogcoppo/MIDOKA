(saldopID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
FECHA DATE,
REFERENCIA INT,
PROOVEDOR INT NOT NULL,
DEBE DOUBLE,
HABER DOUBLE,
UNIQUE index_proovedor (REFERENCIA,PROOVEDOR),
			  CONSTRAINT `PROOVEDOR REMITO`
					FOREIGN KEY (PROOVEDOR) REFERENCES PROOVEDOR (pooID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT

							      
							      ) ENGINE = InnoDB;
		



