(faltanteID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
OPERACION_REFERENCIA_FL INT NOT NULL,
ID_ARTICULO_FL INT NOT NULL,
CANTIDAD_FL INT NOT NULL,
UNIQUE index_FALTANTE (OPERACION_REFERENCIA_FL,ID_ARTICULO_FL,CANTIDAD_FL),
			  CONSTRAINT `OPERACION DE REFERENCIA FALTANTE`
					FOREIGN KEY (OPERACION_REFERENCIA_FL) REFERENCES OPERACIONES (opID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT,
			  CONSTRAINT `ID_FLTICULO FALTANTE`
					FOREIGN KEY (ID_ARTICULO_FL) REFERENCES ARTICULOS (artID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



