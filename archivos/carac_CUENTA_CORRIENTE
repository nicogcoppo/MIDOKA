(cuecoID INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
OPERACION_REF_CC INT NOT NULL,
DEBE DOUBLE,
HABER DOUBLE,
DEVOLUCION DOUBLE,
PERDIDAS DOUBLE,
UNIQUE index_COMISION (OPERACION_REF_CC),
			  CONSTRAINT `CUENTA CORR`
					FOREIGN KEY (OPERACION_REF_CC) REFERENCES OPERACIONES (opID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
