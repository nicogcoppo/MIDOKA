(comiID INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
OPERACION_REF_C INT NOT NULL,
TRABAJADOR INT NOT NULL,
MONTO DOUBLE NOT NULL,
VIGENCIA DATE,
CANCELACION DATE,
TIPO INT NOT NULL,
UNIQUE index_COMISION (OPERACION_REF_C,TRABAJADOR,MONTO,TIPO,VIGENCIA),
			  CONSTRAINT `COMISIONES`
					FOREIGN KEY (OPERACION_REF_C) REFERENCES OPERACIONES (opID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
