(devolID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
INGRESA INT NOT NULL,
CANTIDAD INT NOT NULL,
REFERENCIA_OP INT NOT NULL,
UNIQUE index_DEVOL (INGRESA,REFERENCIA_OP),
			  CONSTRAINT `OPERACION REFERENCIA DEVOL`
					FOREIGN KEY (REFERENCIA_OP) REFERENCES OPERACIONES (opID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT

							      
							      ) ENGINE = InnoDB;
		



