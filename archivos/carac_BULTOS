(bultosID INT NOT NULL PRIMARY KEY AUTO_INCREMENT, 
OPERACION_REF INT NOT NULL,
BULTOS INT NOT NULL,
UNIQUE index_BULTOS (OPERACION_REF),
			  CONSTRAINT `OPERACION DE REFERENCIA BULTOS`
					FOREIGN KEY (OPERACION_REF) REFERENCES OPERACIONES (opID)
						    ON DELETE CASCADE
						       	      ON UPDATE RESTRICT							     
							      
							      ) ENGINE = InnoDB;
		



