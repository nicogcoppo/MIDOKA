SELECT precioID,rubro_rb,clasificacion_cla,mod_md,NOMBRE_COMERCIAL,CODIGO_PROOVEDOR,CODIGO_BARRAS FROM MIDOKA_PGC_B.PRECIOS JOIN (MIDOKA_PGC_B.RUBRO,MIDOKA_PGC_B.CATEGORIA,MIDOKA_PGC_B.MODELO,MIDOKA_PGC_B.PROOVEDOR) ON (MIDOKA_PGC_B.RUBRO.ruID=MIDOKA_PGC_B.PRECIOS.RUBRO_pr AND MIDOKA_PGC_B.CATEGORIA.claID=MIDOKA_PGC_B.PRECIOS.CATEGORIA_pr AND MIDOKA_PGC_B.MODELO.mdID=MIDOKA_PGC_B.PRECIOS.MODELO_pr AND MIDOKA_PGC_B.PROOVEDOR.pooID=MIDOKA_PGC_B.PRECIOS.PROOVEDOR) WHERE clasificacion_cla LIKE '%acr%';
